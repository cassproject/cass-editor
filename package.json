{
  "name": "cass-editor",
  "version": "1.2.28",
  "private": true,
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "serve": "vue-cli-service serve --port=8082 --fix",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "buildDev": "vue-cli-service build --mode development",
    "serveDev": "vue-cli-service serve --port=8082 --mode development",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "/theme.scss",
  "dependencies": {
    "bulma": "^0.9.3",
    "bulma-checkradio": "^1.1.1",
    "bulma-switch": "^2.0.0",
    "cassproject": "^0.5.21",
    "dateformat": "^5.0.1",
    "core-js": "^3.18.1",
    "file-saver": "^2.0.5",
    "jsonld": "^1.8.1",
    "lodash": "^4.17.21",
    "pure-uuid": "^1.6.2",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.12",
    "vue-infinite-loading": "^2.4.5",
    "vue-moment": "^4.1.0",
    "vue-progressbar": "^0.7.5",
    "vue-resource": "^1.5.2",
    "vue-router": "^3.5.1",
    "vue-scrollto": "^2.20.0",
    "vue-server-renderer": "^2.6.12",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.2.1",
    "@vuepress/core": "^1.8.2",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^6.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.11.1",
    "file-loader": "^6.2.0",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.0",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.8.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
