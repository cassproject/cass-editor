    "serve:coverage": "cross-env NODE_ENV=test vue-cli-service serve --port=8082 --fix",
    "test:coverage": "rimraf .nyc_output coverage && start-server-and-test \"npm run serve:coverage\" http://localhost:8082 \"playwright test\" && nyc report",
