{
  "name": "cass-editor",
  "version": "1.6.11",
  "private": true,
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "serve": "vue-cli-service serve --port=8082 --fix",
